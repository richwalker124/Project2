<div id="home">
  <div class="container">
    <div class="row">
      <div class="col-8 offset-2">
        <h1>{{msg}}</h1>
        <h2>Create an Account!</h2>
        <form class="clearfix mb-4" action="POST">

          <div class="form-group">
            <label for="example-text">First Name:</label>
            <input type="text" id="firstName" class="form-control" aria-describedby="example-text" placeholder="John">
          </div>

          <div class="form-group">
            <label for="example-text">Last Name:</label>
            <input type="text" id="lastName" class="form-control" aria-describedby="example-text" placeholder="Doe">
          </div>

          <div class="form-group">
            <label for="example-text">Display Name:</label>
            <input type="text" id="userName" class="form-control" aria-describedby="example-text"
              placeholder="Doeboy86">
          </div>

          <div class="form-group">
            <label for="example-text">Email:</label>
            <input type="text" id="email" class="form-control" aria-describedby="example-text"
              placeholder="jdoe@example.com">
          </div>

          <div class="form-group">
            <label for="example-text">Password:</label>
            <input type="password" id="password" class="form-control" aria-describedby="example-text" placeholder="">
          </div>

          <div class="form-group">
            <label for="example-text">Re-Enter Password:</label>
            <input type="password" id="password-repeat" class="form-control" aria-describedby="example-text"
              placeholder="">
          </div>

          {{!-- Submit Button --}}
          <button id="submit" class="btn btn-primary float-right">Submit</button>
        </form>

        </ul>
      </div>
    </div>
  </div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/index.js"></script>