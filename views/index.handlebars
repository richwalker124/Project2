<div id="home">
  <div class="container">
    <div class="row">
      <div class="col-8 offset-2">
        <h1>{{msg}}</h1>
        <h2>This is our Home Page!</h2>
        <form class="clearfix mb-4" action="POST">

          <div class="form-group">
            <label for="example-text">First</label>
            <input type="text" id="firstName" class="form-control" aria-describedby="example-text"
              placeholder="Enter Example Text">
          </div>


          <div class="form-group">
            <label for="example-text">Last</label>
            <input type="text" id="lastName" class="form-control" aria-describedby="example-text"
              placeholder="Enter Example Text">
          </div>


          <div class="form-group">
            <label for="example-text">Display</label>
            <input type="text" id="userName" class="form-control" aria-describedby="example-text"
              placeholder="Enter Example Text">
          </div>


          <div class="form-group">
            <label for="example-text">Email</label>
            <input type="text" id="email" class="form-control" aria-describedby="example-text"
              placeholder="Enter Example Text">
          </div>

          <div class="form-group">
            <label for="example-text">Password</label>
            <input type="text" id="password" class="form-control" aria-describedby="example-text"
              placeholder="Enter Example Text">
          </div>



          <button id="submit" class="btn btn-primary float-right">Submit</button>
        </form>
